/*
 * Conexio Stratus nRF9160 device overlay configuration for SHT3XD sensor connected to I2C port
 *
 */
&i2c2 {
	status = "okay";
	sht3xd@44 {
		status = "okay";
		compatible = "sensirion,sht3xd";
		reg = <0x44>;
	};
};
